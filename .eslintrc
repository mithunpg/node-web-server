{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "linebreak-style": "off",
    "prefer-destructuring": "off",
    "no-unused-vars": [
      "error",
      {
        "var": "local",
        "args": "none"
      }
    ]
  }
}